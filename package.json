{
  "name": "deltares-ast-2-0",
  "private": true,
  "version": "1.0.0",
  "description": "Lean Web Kit to kick-start your web project.",
  "author": "De Voorhoede (https://www.voorhoede.nl)",
  "homepage": "https://github.com/voorhoede/deltares-ast-2-0#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/deltares-ast-2-0"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "analyze": "nuxt build --analyze",
    "prebuild": "touch ./.env && rimraf dist/ && npm run data",
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "nuxt generate",
    "build:guide": "build-storybook -c config/storybook -o dist/client/guide/",
    "build:server": "netlify-lambda build src/server/functions/",
    "predata": "rimraf src/client/static/data/ src/client/static/_redirects",
    "data": "node ./config/data/_dump-dato.js",
    "postdata": "node ./config/data/_create-redirects.js && node ./config/data/_build-messages.js",
    "predev": "rimraf dist/ && npm run data",
    "dev": "run-p dev:*",
    "dev:client": "PORT=5326 nuxt",
    "dev:server": "netlify-lambda serve src/server/functions/",
    "guide": "start-storybook -p 5327 -c config/storybook -s src/client/static/",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "npm run lint -- --fix",
    "precommit": "npm run lint",
    "proxy": "ngrok http 5326",
    "start": "run-p dev:server start:dist",
    "start:dist": "http-server './dist/client/' -c-1 -o -p 5326 --proxy 'http://localhost:9000/'",
    "new-component": "node ./_scripts/new-component/index.js --component"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "1.0.9",
    "@nuxtjs/proxy": "1.2.4",
    "@nuxtjs/pwa": "2.5.0",
    "@nuxtjs/sitemap": "0.1.1",
    "@turf/area": "^6.0.1",
    "dotenv": "6.0.0",
    "dotenv-safe": "6.0.0",
    "mapbox-gl": "0.48.0",
    "normalize.css": "8.0.0",
    "nuxt": "1.4.2",
    "nuxt-i18n": "5.2.1",
    "query-string": "6.1.0",
    "unfetch": "3.1.1",
    "vue": "2.5.17",
    "vue-carousel": "^0.14.0",
    "vue-material": "1.0.0-beta-10.2",
    "vue-template-compiler": "2.5.17",
    "vue-touch-keyboard": "^0.3.0",
    "vuex-map-fields": "^1.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-knobs": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/addon-options": "3.4.10",
    "@storybook/vue": "3.4.10",
    "autoprefixer": "9.1.3",
    "babel-eslint": "9.0.0",
    "eslint": "5.5.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-vue": "4.7.1",
    "glob": "7.1.3",
    "http-server": "0.11.1",
    "husky": "0.14.3",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "netlify-lambda": "0.4.0",
    "ngrok": "3.0.1",
    "node-fetch": "2.2.0",
    "npm-run-all": "4.1.3",
    "postcss-custom-properties": "7.0.0",
    "postcss-import": "12.0.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "rimraf": "2.6.2",
    "storybook-readme": "3.3.0",
    "vue-svg-loader": "0.8.0"
  }
}
